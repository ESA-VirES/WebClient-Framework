<wps:Execute version="1.0.0" service="WPS" xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1">
  <ows:Identifier>vires:get_model_info</ows:Identifier>
  <wps:DataInputs>{{#if model_ids}}
    <wps:Input>
      <ows:Identifier>model_ids</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{model_ids}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>{{/if}}{{#if shc}}
    <wps:Input>
      <ows:Identifier>shc</ows:Identifier>
      <wps:Data>
        <wps:ComplexData>{{shc}}</wps:ComplexData>
      </wps:Data>
    </wps:Input>{{/if}}
  </wps:DataInputs>
  <wps:ResponseForm>
    <wps:RawDataOutput {{#if mimeType}}mimeType="{{mimeType}}"{{/if}}>
      <ows:Identifier>output</ows:Identifier>
    </wps:RawDataOutput>
  </wps:ResponseForm>
</wps:Execute>
